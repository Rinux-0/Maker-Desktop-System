choice
	prompt "test"
	depends on TEST
	default TEST_KEYTEST

	config TEST_KEYTEST
		bool "keytest"
		select COMM_FORMAT_HID
		select COMM_SPI
		select COMM_UART
		select COMM_SLE
		select COMM_SLE_SERVER
		select COMM_WIFI
		help
			[spi] & [uart] & [sle_s]

	menuconfig TEST_TMPTEST
		bool "tmptest"
		orsource "./tmptest/Kconfig"
endchoice
