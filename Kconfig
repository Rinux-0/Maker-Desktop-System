config ENABLE_MAKER_DESKTOP_SAMPLE
    bool "Enable the Sample of maker-desktop"
    default y
    depends on SAMPLE_ENABLE
if ENABLE_MAKER_DESKTOP_SAMPLE
    osource "application/samples/maker-desktop/Kconfig"
endif

config ENABLE_KEYPAD_SAMPLE
    bool "Enable the Sample of keypad"
    default n
    depends on SAMPLE_ENABLE
if ENABLE_KEYPAD_SAMPLE
    osource "application/samples/keypad/Kconfig"
endif

config ENABLE_KEYPAD_1V8_SAMPLE
    bool "Enable the Sample of keypad_1v8"
    default n
    depends on SAMPLE_ENABLE
if ENABLE_KEYPAD_1V8_SAMPLE
    osource "application/samples/keypad_1v8/keypad/Kconfig"
    osource "application/samples/keypad_1v8/sle_uart_1v8/Kconfig"
endif



config ENABLE_PERIPHERAL_SAMPLE
    bool
    prompt "Enable the Sample of peripheral."
    default n
    depends on SAMPLE_ENABLE
    help
        This option means enable the sample of peripheral.

if ENABLE_PERIPHERAL_SAMPLE
osource "application/samples/peripheral/Kconfig"
endif

